@article{scottInputDesignGuaranteed2014,
  title = {Input Design for Guaranteed Fault Diagnosis Using Zonotopes},
  author = {Scott, Joseph K. and Findeisen, Rolf and Braatz, Richard D. and Raimondo, Davide M.},
  date = {2014-06},
