@article{lindbeckPredictionsThumbHand2023,
  title = {Predictions of Thumb, Hand, and Arm Muscle Parameters Derived Using Force Measurements of Varying Complexity and Neural Networks},
  author = {Lindbeck, Erica M. and Diaz, Maximillian T. and Nichols, Jennifer A. and Harley, Joel B.},
