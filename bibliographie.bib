@article{sobinovApproximatingComplexMusculoskeletal2020,
  title = {Approximating Complex Musculoskeletal Biomechanics Using Multidimensional Autogenerating Polynomials},
  author = {Sobinov, Anton and Boots, Matthew T. and Gritsenko, Valeriya and Fisher, Lee E. and Gaunt, Robert A. and Yakovenko, Sergiy},
  editor = {Vanderelst, Dieter},
  year = {2020},
  month = dec,
  journal = {PLOS Computational Biology},
  volume = {16},
