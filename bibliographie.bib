  pmid = {30087262},
  keywords = {Adult,Australia,biomechanics,Cognition,Computers,Ergonomics,Female,human-computer interaction,Humans,Male,mental work capacity,Middle Aged,musculoskeletal disorders,office ergonomics,Postural Balance,Sitting Position,Work,Workplace,Young Adult},
